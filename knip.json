{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    "apps/client": {
      "entry": ["app/**/*.{ts,tsx}", "**/*.@(ios|android|web).@(ts|tsx)"],
      "ignore": ["babel.config.js"],
      "ignoreDependencies": ["@babel/runtime", "expo-updates"]
    }
  },
  "rules": {
    "duplicates": "warn",
    "binaries": "warn"
  }
}
